/// A day of the week
enum Weekday {
  MONDAY;
  TUESDAY;
  WEDNESDAY;
  THURSDAY;
  FRIDAY;
  SATURDAY;
  SUNDAY;
}

/// A JSON value
enum JsonValue {
  NULL = 1;
  boolean: bool = 100;
  number: float64 = 6;
  string: string = 3;
  array: [JsonValue] = 4;
  struct Pair {
    name: string = 0;
    value: JsonValue = 1;
  }
  /// A list of
  /// name:value pairs
  object: [Pair|name] = 5;
}

enum EmptyEnum {}

enum EnumWithNameConflict {
  SERIALIZER;
}

struct WeekdayHolder {
  weekday: Weekday;
}
